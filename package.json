{
  "name": "tempart",
  "version": "0.3.0",
  "description": "Its a mustache templating engine, which is optimized for recompiling. The parser returns a tree of blocks with each variable dependency",
  "main": "src/precompiler/precompiler.js",
  "scripts": {
    "test": "jshint --show-non-errors src && ./node_modules/karma/bin/karma start karma.conf.js --no-auto-watch --single-run"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/plusgut/tempart.git"
  },
  "keywords": [
    "template",
    "recompile",
    "blocks"
  ],
  "author": "Carlo jeske",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/plusgut/tempart/issues"
  },
  "jshintConfig": {
    "node": true,
    "esversion": 6,
    "loopfunc": true,
    "unused": true,
    "undef": true,
    "curly": true,
    "eqeqeq": true,
    "forin": true
  },
  "devDependencies": {
    "babel-plugin-transform-es2015-modules-amd": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "grunt": "^1.0.1",
    "grunt-babel": "^6.0.0",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "load-grunt-tasks": "^3.5.0",
    "karma": "^1.1.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.0"
  }
}
